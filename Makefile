# -fsanitize=address -g -Wall -Wextra -Werror`


NAME = push_swap
NAME2 = checker
CC = gcc
FLAGS = -Wall -Wextra -Werror
SRCS =		main.c\
			comands_01.c\
			comands_02.c\
			comands_03.c\
			ft_io.c\
			ft_oi.c\
			ft_man.c\
			p_swap.c\
			parser.c\
			prosto.c\
			utils_2.c\
			utils.c\
			#philo.h
SRCS_B =	main_bonus.c\
			exit_bonus.c\
			utils_bonus.c
OBJS = $(SRCS:.c=.o)

OBJS_B = $(SRCS_B:.c=.o)

%.o: %.c
	$(CC) $(FLAGS) -o $@ -c $<

all:	$(NAME)

$(NAME): $(OBJS)
		$(CC) $(FLAGS) $(SRCS) -o $(NAME)
bonus: $(OBJS_B)
		$(CC) $(FLAGS) $(SRCS) -o $(NAME2)

clean:
		rm -f $(OBJS) $(OBJS_B) a.out

fclean:		clean
		rm -f $(NAME) $(NAME2)

re:		fclean all

.PHONY: all clean fclean re bonus
